  <name>MaPSeq :: Pipelines :: GS :: Merge VC :: Executor</name>
  <modelVersion>4.0.0</modelVersion>
  <groupId>edu.unc.mapseq.pipelines.gs-merge-vc</groupId>
  <artifactId>gs-merge-vc-executor</artifactId>
  <version>0.6.47-SNAPSHOT</version>
  <packaging>bundle</packaging>
  <properties>
    <maven.site.plugin.skip>true</maven.site.plugin.skip>    
  </properties>                        
  <dependencies>
    <dependency>
      <groupId>edu.unc.mapseq.pipelines.gs-merge-vc</groupId>
      <artifactId>gs-merge-vc-workflow</artifactId>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
	<groupId>org.apache.felix</groupId>
	<artifactId>maven-bundle-plugin</artifactId>
	<configuration>
	  <instructions>
	    <Private-Package />
	    <Export-Package>edu.unc.mapseq.executor.gs.mergevc.*</Export-Package>
	    <Import-Package>*</Import-Package>
	  </instructions>
	</configuration>
      </plugin>
    </plugins>
  </build>
</project>
